## WordPress site base URL
#
# For local dev without CORS hassles, you can leave this empty and Vite will proxy
# /wp-json/* to the target set in vite.config.ts.
VITE_WP_BASE_URL=""

## App Connector keys
#
# X-App-Key for GET /auth (required when auth_required=1)
VITE_APP_API_KEY=""

# Optional: X-Admin-Key for /admin/pending-count badge
VITE_ADMIN_KEY=""
